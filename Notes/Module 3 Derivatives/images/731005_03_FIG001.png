<div><a href="#popup-signin" id="popup-signin">Click to Sign In</a></div>

<script type="text/javascript">
  var watchForPopupClose = function(popup, callback){
    var checkIt = window.setInterval(function(){
      if(popup.closed !== false){
        window.clearInterval(checkIt);
        callback();
      }
    }, 200);
  };

  $(document).ready(function(){
    VST.Jigsaw.trigger('viewer:unauthorized');
  });
  $('#popup-signin').click(function(e){
    e.preventDefault();

    var url        = '/login'
      , windowName = 'Login'
      , extra      = 'toolbar=no, location=no, directories=no, status=no, menubar=no, scrollbars=no, resizable=no, copyhistory=no, height=500, width=600';

    var popup;

    if(typeof VST !== 'undefined' && typeof VST.Jigsaw !== 'undefined'){
      VST.Jigsaw.socket.topURL(function(topURL){
        url += '&from='+topURL;
        popup = window.open(url, windowName, extra);
        watchForPopupClose(popup, function(){
          VST.Jigsaw.trigger('viewer:recreate');
        });
      });
    }else{
      popup = window.open(url, windowName, extra);
      watchForPopupClose(popup, function(){
        window.location.reload();
      });
    }
  });
</script>

